<h1>FullCalls#index</h1>
<p>Find me in app/views/full_calls/index.html.erb</p>

<center>
<h1>WORDS YOU GUYS WANT TO KNOW</h1>
<table>
  <tbody>
    <% @full_calls.in_groups_of(4, false) do |row| %>
      <tr>
        <% for word in row %>
          <td><pre>

          <%= link_to "#{word}", full_calls_path(full_call: {word: "#{word}" }), method: 'post' %>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</center>
