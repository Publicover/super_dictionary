<center>
<p>
  <div>
    <h2 id='full-call-heading'><%= @full_call.word.upcase %></h2>
    <div id='full-call-alert'>Thanks for voting!</div>
  </div>
</p>


<div class="container">
  <div class="card-title">
    <div class="card card-default">
      <div class="card-header"><strong>Oxford Definition:</strong>
        <div>
          <button type="button" disabled>VOTE OXFORD</button>
        </div>
      </div>
    <div class="card-block"><%= @full_call.oxford_def %></div>

    <div class="card-header"><strong>Webster Definition:</strong>
      <div>
         <button type="button" disabled>VOTE WEBSTER</button>
      </div>
    </div>
    <div class="card-block"><%= @full_call.webster_def %></div>

    <div class="card-header">
      <strong>Johnson Definition:</strong>
      <div>
          <button type="button" disabled>VOTE JOHNSON</button>
        </div>
      </div>
    <div class="card-block"><%= @full_call.johnson_def %></div>

    <div class="card-header">
      <strong>Urban Definition:</strong>
        <div>
          <button type="button" disabled>VOTE URBAN</button>
        </div>
      </div>
    <div class="card-block"><%= @full_call.urban_def %></div>
    </div>
  </div>
</br>
<p><%= link_to 'Make another word', root_path %></p>
<p><%= link_to 'See all words', full_calls_path %></p>
</center>



