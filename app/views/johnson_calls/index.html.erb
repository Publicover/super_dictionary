<h1>JohnsonCalls#index</h1>
<p>Find me in app/views/johnson_calls/index.html.erb</p>

<center>
<h1>Listing Words</h1>

<table>
  <tr>
    <th>Word</th>
    <th>Definition</th>
    <th>File Type</th>
  </tr>

  <% @johnson_calls.each do |word| %>
    <tr>
      <td><%= word.word %></td>
      <td><%= word.definition %></td>
      <td><%= (word.definition).class %></td>
      <td><%= link_to 'Show', johnson_call_path(word) %></td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to 'Make a new Johnson word', new_johnson_call_path %>
</center>
